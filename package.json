  "bugs": {
    "url": "https://github.com/colbyfayock/pagespeed-insights-report-action/issues"
  },
  "homepage": "https://github.com/colbyfayock/pagespeed-insights-report-action#readme",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "fs": "^0.0.1-security",
    "psi": "^4.0.1"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.22.1"
  }
}
